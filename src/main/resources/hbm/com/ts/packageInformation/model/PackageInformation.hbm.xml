<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.gr.notification.model.Profile" table="PackageInformation">
		<id column="Id" name="id" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property column="Name" name="name" type="java.lang.String" />
		<property column="Price" name="price" type="java.lang.Integer"/>
		<property column="Active" name="active" type="java.lang.Boolean"/>
		<property column="Description" name="description" type="java.lang.String" />
		<property column="Password" name="password" type="java.lang.String" />
		<property column="Duration" name="duration" type="java.lang.Integer" />
		<property column="CreateDateTime" name="createDateTime" type="java.util.Date" />
		<property column="ModifiedDateTime" name="modifiedDateTime" type="java.util.Date" />
	</class>
</hibernate-mapping>